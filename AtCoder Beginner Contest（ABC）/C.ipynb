   "source": [
    "import numpy as np\n",
    "N, K, Q = map(int, input().split())\n",
    "score = np.array([K]*N)\n",
    "\n",
    "for _ in range(Q):\n",
    "    n = int(input())\n",
    "    score[n-1] = score[n-1] + 1\n",
    "\n",
    "score=score-Q\n",
    "\n",
    "for i in score:\n",
    "    print(\"Yes\" if i>0 else \"No\")"
   ]
