    "N = int(input())\n",
    "\n",
    "def power(num):\n",
    "    if num==N:\n",
    "        return num\n",
    "    return num*power(num+1)\n",
    "\n",
    "if N==1:\n",
    "    print(1)\n",
    "\n",
    "print(power(1))"
